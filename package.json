  "repository": {
    "type": "git",
    "url": "git+https://github.com/guillaumemorin/react-styled-toggle.git"
  },
  "bugs": {
    "url": "https://github.com/guillaumemorin/react-styled-toggle/issues"
  },
  "homepage": "https://github.com/guillaumemorin/react-styled-toggle#readme",
  "keywords": [
    "react",
    "toggle",
    "button",
    "typescript",
    "styled-components"
